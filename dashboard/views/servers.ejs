    }
    
    
    @keyframes animatedBackground {
    
      from { background-position: bottom 10px 0px; }
      to { background-position: 10000000% 0; }
    }
    
    body	{ 
        background-image: url("./img/background.png");
      background-position: bottom 10px 0px;
